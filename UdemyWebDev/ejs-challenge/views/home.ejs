<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%= startContent %></p>
<% posts.forEach(function(post){ %>
  <h1><%= post.postTitle %></h1>
  <% let link = "/posts/" + post.postTitle%>

  <p><%= post.postBody.substring(0, 100) + "..." %>
    <a href=<%= link %>>Read More</a>
  </p>
<% }); %>

<%- include("partials/footer"); -%>